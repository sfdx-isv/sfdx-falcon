{
  "name": "sfdx-falcon",
  "description": "A plugin for the Salesforce CLI that enables and enhances implementation of the SFDX-Falcon template.",
  "version": "0.0.94",
  "author": "Vivek M. Chawla @VivekMChawla",
  "bugs": "https://github.com/sfdx-isv/sfdx-falcon/issues",
  "dependencies": {
    "@oclif/command": "1.5.18",
    "@oclif/config": "1.13.2",
    "@oclif/errors": "1.2.2",
    "@salesforce/command": "2.1.0",
    "@salesforce/core": "2.1.1",
    "chalk": "2.4.2",
    "csv-parser": "2.3.0",
    "debug": "4.1.1",
    "falcon-listr-update-renderer": "0.4.2",
    "inquirer": "6.5.0",
    "json2csv": "4.5.2",
    "listr": "0.14.3",
    "lodash": "4.17.15",
    "pad": "3.2.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "rxjs": "6.5.2",
    "sfdx-falcon-appx-demo-kit": "0.0.92",
    "sfdx-falcon-appx-package-kit": "0.0.5",
    "shelljs": "0.8.3",
    "tslib": "1.10.0",
    "unzipper": "0.10.3",
    "uuid": "3.3.2",
    "yeoman-environment": "2.4.0",
    "yeoman-generator": "4.0.1",
    "yosay": "2.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/plugin-help": "2.2.0",
    "@oclif/test": "1.2.5",
    "@salesforce/dev-config": "1.5.0",
    "@types/chai": "^4.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jsforce": "^1.9.9",
    "@types/json2csv": "^4.5.0",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/read-pkg": "^5.1.0",
    "@types/shelljs": "^0.8.5",
    "@types/yeoman-environment": "^2.3.2",
    "@types/yeoman-generator": "^3.1.4",
    "@types/yosay": "^0.0.29",
    "chai": "^4.2.0",
    "cross-spawn": "^6.0.5",
    "del": "^5.0.0",
    "globby": "^10.0.1",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "falcon": {
    "sfdcApiVersion": "45.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    ".oclif.manifest.json"
  ],
  "homepage": "https://sfdx-isv.github.io/sfdx-falcon/",
  "keywords": [
    "salesforce",
    "sfdx",
    "sfdx-falcon",
    "sfdx-plugin",
    "appexchange",
    "appexchange-demo-kit",
    "appexchange-package-kit",
    "adk",
    "apk"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "adk": {
        "description": "AppExchange Demo Kit commands"
      },
      "apk": {
        "description": "AppExchange Package Kit commands"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sfdx-isv/sfdx-falcon",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "singleTest": "nyc --extension .ts mocha --forbid-only \"test/**/adk/create.test.ts\"",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
